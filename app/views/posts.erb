<div class="container">
  <div class="hero-unit">
    <h1>All Yer Posts</h1>
  </div>
  <%=erb :_tags, :layout => false %>
  <div class="row-fluid">
    <%Post.all.reverse.each do |p| %>
      <div class="span4">
        <h2 class="form-signin-heading">
          <a href=/<%=p.id %>><%=p.title %></a>
        </h2>
        <p><%=p.body %></p>
      </div>
    <% end %>
  </div>        
</div>

